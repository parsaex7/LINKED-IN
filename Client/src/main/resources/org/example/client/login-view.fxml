<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="670.0" prefWidth="950.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.client.logInViewController">
   <children>
      <Label layoutX="377.0" layoutY="53.0" text="Log IN">
         <font>
            <Font name="System Bold Italic" size="64.0" />
         </font>
      </Label>
      <Label layoutX="365.0" layoutY="187.0" text="please enter your Email : ">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TextField fx:id="email" layoutX="401.0" layoutY="268.0" />
      <Label layoutX="300.0" layoutY="326.0" text="Please choose a password for yourself : ">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TextField fx:id="passWord" layoutX="402.0" layoutY="400.0" />
      <Button fx:id="done" layoutX="449.0" layoutY="467.0" mnemonicParsing="false" onAction="#onDone" text="Send" />
      <Label fx:id="result" layoutX="395.0" layoutY="548.0" textFill="#df0707">
         <font>
            <Font size="18.0" />
         </font></Label>
   </children>
</AnchorPane>
